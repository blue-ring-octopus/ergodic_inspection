<launch>
  <node pkg="nodelet" type="nodelet" name="nodelet_manager" args="manager" />
  <node pkg="nodelet" type="nodelet" name="nodelet1"
        args="load depth_image_proc/point_cloud_xyzrgb nodelet_manager">
    <remap from="rgb/camera_info" to="/camera/rgb/camera_info"/>
    <remap from="depth_registered/image_rect" to="/camera/depth_registered/image_raw"/>
    <remap from="rgb/image_rect_color" to="/camera/rgb/image_rect_color"/>
  </node>

<node pkg="ergodic_inspection" name="anomaly_detector_ros" type="anomaly_detector_ros.py" output="screen">
  </node> 

<node pkg="ergodic_inspection" name="map_manager" type="map_manager_ros.py" output="screen">
  </node> 
</launch>
